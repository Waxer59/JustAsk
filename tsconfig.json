{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@components/*": ["./src/components/*"],
      "@layouts/*": ["./src/layouts/*"],
      "@shadcn/*": ["./src/shadcn"],
      "@lib/*": ["./src/lib"],
      "@styles/*": ["./src/styles/*"],
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
